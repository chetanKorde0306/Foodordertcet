        }
        <style>
        *{
            font-family: 'Nunito', sans-serif;
        }
        h2{
            color: white;
            font-size: 3.5rem;
        }
        .container{
            display :flex;
            align-items: center;
            justify-content: center;
            height: 100%;
            /* height: 500px; */
        }
        .container .form{
            width: 400px;
            height: 300px;
            border: 1px solid black;
            border-top: 5px solid red;
        }
        .form table tr td{
            padding: 15px;
            
        }
        .form h2,.form button{
            padding-left: 15px;
        }
        .btn{
            margin: 15px;
    width: 105px;
    background: red;
    border: none;
    border: 1px solid black;
    padding: 5px;
    color: white;
    border-radius: 10px;
        }
        input{
          
    border: none;
    padding-bottom: 6px;
    border-bottom: 1px solid black;
    width: 225px;
    background: rgb(247, 178, 178);
        }
        td{
            font-size: 1.5rem;
            color: #fff;
        }

    </style>
